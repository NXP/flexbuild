--- a/arch/arm/dts/Makefile
+++ b/arch/arm/dts/Makefile
@@ -956,6 +956,7 @@ dtb-$(CONFIG_ARCH_IMX8M) += \
 	imx8mp-dhcom-pdk3-overlay-rev100.dtbo \
 	imx8mp-dhcom-picoitx.dtb \
 	imx8mp-ddr4-evk.dtb \
+	imx8mp-frdm.dtb \
 	imx8mp-icore-mx8mp-edimm2.2.dtb \
